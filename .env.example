# Microservice port
PORT=3000

# File paths
PROXY_FILE=proxies.txt
USER_AGENT_FILE=user-agents.txt
RETRY_COUNT=3
# Output and scraping configuration
OUTPUT_FILE=scraped_page.html
DELAY_MIN=5
DELAY_MAX=10

# API key for authentication
API_KEY=your_secure_api_key

# Additional configurations
USER_AGENT_ROTATION_INTERVAL=5000
CAPTCHA_API_KEY=your_captcha_api_key

# Proxy configuration
USE_PROXIES=true
# Ips allowed to access the microservice
ALLOWED_IPS=127.0.0.1,192.168.1.1

CHROME_BIN_PATH=/usr/bin/google-chrome